{
  "name": "starter-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass --watch styles/scss/main.scss public/styles/main.css --output-style",
    "scss-min": "node-sass --watch styles/scss/main.scss public/styles/main-min.css --output-style compressed",
    "postcss": "postcss public/styles/main.css --replace --use autoprefixer",
    "build": "node-sass styles/scss/main.scss public/styles/main.css --output-style compressed && postcss public/styles/main.css --replace --use autoprefixer",
    "dev": "node-sass --watch styles/scss/main.scss public/styles/main.css --output-style compressed && postcss public/styles/main.css --watch --replace --use autoprefixer --watch"
  },
